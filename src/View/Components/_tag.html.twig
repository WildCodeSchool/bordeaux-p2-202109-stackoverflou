<div class="container pt-3 pb-3 mx-auto">
    <div class="row mt-2">
        <div class="text-center">
            {% for tag in tags %}
                <a href="/questions/tags?id={{ tag.id }}" type="button" class="btn btn-outline-{{ tag.color}} mb-2 ms-2">
                    {{ tag.name }}
                </a>
            {% else %}
                Aucun tag
            {% endfor %}
        </div>
    </div>
</div>

